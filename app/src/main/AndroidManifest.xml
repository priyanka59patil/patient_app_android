<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.werq.patient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.ScheduleDetailsActivity"
            android:label="@string/title_activity_schedule_details"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Activities.DemoActivity" />
        <activity
            android:name=".Activities.FingerPrintActivity"
            android:label="@string/title_activity_finger_print"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.SettingActivity"
            android:label="@string/title_activity_setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.FunctionalActivity" />
        <activity android:name=".Activities.ImmunizationResultsActivity" />
        <activity android:name=".Activities.SummaryCareActivity" />
        <activity android:name=".Activities.ProfileActivity" />
        <activity
            android:name=".Activities.CreateAccountActivity"
            android:label="@string/title_activity_create_account"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.VerifyIdentity"
            android:label="@string/title_activity_verify_identity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.SignUpActivity"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.BottomTabActivity"
            android:label="@string/title_activity_bottom_tab" />
        <activity
            android:name=".Activities.ForgotPasswordActivity"
            android:label="@string/title_activity_forgot_password"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.SplashActivity"
            android:label="@string/title_activity_splash"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>